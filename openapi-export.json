{
  "info": {
    "description": "Get weather information for a location",
    "title": "get-weather",
    "version": "1.0.0"
  },
  "openapi": "3.0.0",
  "paths": {
    "https://api.openweathermap.org/data/2.5/weather": {
      "get": {
        "description": "Get weather information for a location",
        "operationId": "get-weather",
        "parameters": [
          {
            "description": "City name",
            "in": "query",
            "name": "q",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "API key",
            "in": "query",
            "name": "appid",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "example": {
                  "main": {
                    "humidity": 75,
                    "temp": 293.15
                  },
                  "weather": [
                    {
                      "description": "clear sky",
                      "main": "Clear"
                    }
                  ]
                },
                "schema": {
                  "type": "object"
                }
              }
            },
            "description": "Weather information"
          }
        },
        "summary": "Get weather information for a location"
      }
    }
  }
}